  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.27.2",
    "eslint": "^9.9.1",
    "typescript": "^5.5.3"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@algolia/client-search",
        "@types/react",
        "react",
        "vite",
        "@types/markdown-it",
        "vue",
        "react-dom"
      ]
    }
